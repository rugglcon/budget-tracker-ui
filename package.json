{
  "name": "budget-tracker",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --source-map",
    "test": "jest",
    "lint": "ng lint",
    "build:prod": "ng build --prod --progress=false --aot --sourceMap=false --optimization",
    "start:prod": "ng serve --host 0.0.0.0 --configuration=production",
    "ci": "npm run lint && npm run test && npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.4",
    "@angular/common": "^9.1.4",
    "@angular/compiler": "^9.1.4",
    "@angular/core": "^9.1.4",
    "@angular/forms": "^9.1.4",
    "@angular/localize": "^9.1.4",
    "@angular/platform-browser": "^9.1.4",
    "@angular/platform-browser-dynamic": "^9.1.4",
    "@angular/router": "^9.1.4",
    "@ng-bootstrap/ng-bootstrap": "^6.1.0",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.5",
    "crypto": "^1.0.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.0",
    "popper.js": "^1.16.1",
    "rxjs": "^6.5.5",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.4",
    "@angular/cli": "^15.1.4",
    "@angular/compiler-cli": "^9.1.4",
    "@angular/language-service": "^9.1.4",
    "@types/hammerjs": "^2.0.36",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "codelyzer": "^5.2.2",
    "jest": "^25.5.4",
    "jest-preset-angular": "^8.1.3",
    "ts-node": "^8.9.1",
    "tslint": "^6.1.2",
    "typescript": "3.8.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__test__/.*|(\\./)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/src/tsconfig.spec.json"
      }
    },
    "testPathIgnorePatterns": [
      "<rootDir>/src/test.ts"
    ]
  }
}
